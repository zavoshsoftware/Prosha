
@{
    ViewBag.Title = "ورود";
    Layout = "~/Views/Shared/FrontEnd/_FronEndLayout.cshtml";
}
<section id="page-title" class="page-title">
    <div class="container-fluid bg-overlay bg-white">
        <div class="bg-section">
            <img src="/assets/images/page-title/title-20.jpg" alt="Background" />
        </div>
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12">
                <div class="title title-1 text--center">
                    <div class="title--content">
                        <div class="title--heading">
                            <h1>@Resources.Label.Login</h1>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <ol class="breadcrumb">
                        <li><a href="/">@Resources.Label.Home</a></li>
                        <li class="active">@Resources.Label.Login</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</section>

<div class="row">
    <div class="container">
        <div class="col-md-6 col-md-push-3">
            <div class="login-form">
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
</div>


